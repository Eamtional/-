<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
	"http://struts.apache.org/dtds/struts-2.5.dtd">

<!-- START SNIPPET: xworkSample -->
<struts>  
    <constant name="struts.enable.DynamicMethodInvocation" value="true" />
    <constant name="struts.devMode" value="true" />   
    
    <package name="Class" extends="struts-default" namespace="/Class">
    	<action name="Class">
    	<result>/WEB-INF/content/Admin/Class.jsp</result>
    	</action>
    	 <action name="select" class="com.dzf.ssxk.SH.action.ClassAction" method="select"></action>
    	 <action name="sel" class="com.dzf.ssxk.SH.action.ClassAction" method="sel"></action>
    	 <action name="insert" class="com.dzf.ssxk.SH.action.ClassAction" method="insert">
    		<result name="success">/WEB-INF/content/Admin/Class.jsp</result>
    		<result name="error">/WEB-INF/content/Admin/Class.jsp</result>
    	</action> 
    	<action name="delete" class="com.dzf.ssxk.SH.action.ClassAction" method="delete">
    		<result name="success">/WEB-INF/content/Admin/Class.jsp</result>
    		<result name="error">/WEB-INF/content/Admin/Class.jsp</result>
    	</action>  
    	 
    </package>
   
 
    
 </struts>



